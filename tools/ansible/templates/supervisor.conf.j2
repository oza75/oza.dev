[program:{{domain_name}}-worker]
process_name=%(program_name)s_%(process_num)02d
command=php {{ansistrano_deploy_to}}/current/artisan queue:work --queue=high,medium,default,low --timeout=0 --tries=3
autostart=true
autorestart=true
user=www-data
#user=ubuntu
numprocs=4
redirect_stderr=true
stdout_logfile=/var/log/{{domain}}/worker.log
stopwaitsecs=3600
